#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/dprint@0.49.1/node_modules/dprint/node_modules:/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/dprint@0.49.1/node_modules:/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/dprint@0.49.1/node_modules/dprint/node_modules:/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/dprint@0.49.1/node_modules:/Users/samdevo/Code/DefinitelyTyped/types/react-relay/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
